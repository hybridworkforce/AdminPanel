            <div class="content">

                <!-- Wizard with validation -->
                <div class="card">
                    <div class="card-header bg-white header-elements-inline">
                        <h6 class="card-title">form</h6>
                        <div class="header-elements">
                            <div class="list-icons">
                                <a class="list-icons-item" data-action="collapse"></a>
                                <a class="list-icons-item" data-action="reload"></a>

                            </div>
                        </div>
                    </div>

                    <div class="card-body">
                        <form class="form-validate-jquery">
                            <h6>Supplier Details</h6>

                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Supplier Name<span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <input type="text" name="name" ng-model="supplieronboard.supplier_name" class="form-control " required placeholder="Your Name">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Account Number<span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <input type="text" name="name" ng-model="supplieronboard.account_number" class="form-control " required placeholder="Your Account Number">
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Email <span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <input type="email" name="email" ng-model="supplieronboard.email" class="form-control " id="email" required placeholder="Enter a valid email address">
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Address: <span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <input type="text" name="Address" ng-model="supplieronboard.address" class="form-control" id="address" required placeholder="Your Address">
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Document Types <span class="text-danger">*</span></label>
                                <div class="col-lg-2">
                                    <select name="styled_select" ng-model="supplieronboard.document_types" class="form-control select-icons" data-fouc>
                                        <option value="">Choose an option</option>
                                        <option value="Pdf" data-icon="file-pdf">PDF</option>
                                        <option value="Word" data-icon="file-word">Word</option>
                                        <option value="Excel" data-icon="file-excel">Excel</option>
                                        <option value="Openoffice" data-icon="file-openoffice">Open office</option>
                                    </select>
                                </div>
                            </div>
                           
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">File uploader <span class="text-danger">*</span></label>
                                <div class="col-lg-2">
                                    <input type="file" name="file_uploder" file-model class="form-control" required>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Comments <span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <textarea rows="5" cols="5" ng-model="supplieronboard.comments" name="textarea" class="form-control" placeholder="Comments"></textarea>
                                </div>
                            </div>

                            <h6>Business Rules</h6>
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Rules Types <span class="text-danger">*</span></label>
                                <div class="col-lg-2">
                                    <select name="styled_select" ng-model="supplieronboard.rules_type" class="form-control select-icons" data-fouc>
                                        <option value="">Rules Type</option>
                                        <option value="Genaral" data-icon="#">Genaral</option>
                                        <option value="Universal" data-icon="#">Universal</option>

                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Rules<span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <textarea rows="5" cols="5" ng-model="supplieronboard.rules" name="textarea" class="form-control" required placeholder="1.Business Rule"></textarea>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">signature By<span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <input type="text" name="signature By" ng-model="supplieronboard.signature_by" class="form-control" required placeholder="Signature By">
                                </div>
                            </div>

                            <!--<button type="submit" class="btn btn-primary ml-3" value="{{btntext}}" ng-click="savedata()">Submit <i class="icon-paperplane ml-2"></i></button>-->
                            <div class="d-flex justify-content-end align-items-center">
                                <button type="reset" class="btn btn-light" id="reset">Reset <i class="icon-reload-alt ml-2"></i></button>
                                <button type="submit" value="{{btntext}}" ng-click="savedata()" class="btn btn-primary ml-3">Submit <i class="icon-paperplane ml-2"></i></button>
                            </div>


                        </form>


