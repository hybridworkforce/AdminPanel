                <!-- Wizard with validation -->
                <div class="card">
                    <div class="card-header bg-white header-elements-inline">
                        <h6 class="card-title">Update User Details</h6>
                        <div class="header-elements">
                            <div class="list-icons">
                                <a class="list-icons-item" data-action="collapse"></a>
                                <a class="list-icons-item" data-action="reload"></a>
                                <a class="list-icons-item" data-action="remove"></a>
                            </div>
                        </div>
                    </div>
                    <div class="card-body" ng-app="SingupApp" ng-controller="SingupController" data-ng-init="loadrecord(@Convert.ToInt32(Request.QueryString["id"]))">
                        <form>
                            <h6></h6>
                            <!--user name-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">User Name<span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <input type="text" ng-model="registration.user_name" class="form-control " required placeholder="Enter Your User Name">
                                </div>
                            </div>
                            <!--first name-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">First Name<span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <input type="text" ng-model="registration.first_name" class="form-control " required placeholder="Enter Your First Name">
                                </div>
                            </div>
                            <!--last name-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Last Name<span class="text-danger"></span></label>
                                <div class="col-lg-9">
                                    <input type="text" ng-model="registration.last_name" class="form-control "required placeholder="Enter Your Last Name">
                                </div>
                            </div>
                            <!--email-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Email <span class="text-danger"></span></label>
                                <div class="col-lg-9">
                                    <input type="email" ng-model="registration.email" class="form-control " required placeholder="Enter a valid email address">
                                </div>
                            </div>
                            <!--password-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Password<span class="text-danger"></span></label>
                                <div class="col-lg-9">
                                    <input type="password" ng-model="registration.password" class="form-control "  required placeholder="Enter Your Password">
                                </div>
                            </div>
                            <!--address-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Address : <span class="text-danger"></span></label>
                                <div class="col-lg-9">
                                    <input type="text" ng-model="registration.address" class="form-control" required placeholder="Enter a contact Number">
                                </div>
                            </div>
                            <!--mobile-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Mobile : <span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <input type="text" ng-model="registration.mobile" class="form-control" required placeholder="Enter Your Address">
                                </div>
                            </div>
                            <!--mobile-->
                            <!--country-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Country : <span class="text-danger">*</span></label>
                                <div class="col-lg-9">
                                    <input type="text" ng-model="registration.country" class="form-control" required placeholder="Enter Your Country">
                                </div>
                            </div>
                            <!--state-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">State : <span class="text-danger"></span></label>
                                <div class="col-lg-9">
                                    <input type="text" ng-model="registration.state" class="form-control" required placeholder="Enter Your state">
                                </div>
                            </div>
                            <!--city-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">City : <span class="text-danger"></span></label>
                                <div class="col-lg-9">
                                    <input type="text" name="City" ng-model="registration.city" class="form-control"  required placeholder="Enter Your city">
                                </div>
                            </div>
                            <!--user role id-->
                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">Role Types <span class="text-danger"></span></label>
                                <div class="col-lg-2">
                                    <select name="styled_select" ng-model="registration.user_role_id" class="form-control select-icons" data-fouc>
                                        <option value="">Choose an option</option>
                                        <option value="1" data-icon="icon-user-plus">Project Owner</option>
                                        <option value="2" data-icon="icon-spinner3">Process Owner</option>
                                        <option value="3" data-icon="icon-user">Clerk</option>

                                    </select>
                                </div>
                            </div>
                            <div class="d-flex justify-content-end align-items-center">
                                <button type="reset" class="btn btn-light" id="reset">Reset <i class="icon-reload-alt ml-2"></i></button>
                                <button type="submit" value="{{btntext}}" ng-click="updatedata()" class="btn btn-primary ml-3">Submit <i class="icon-paperplane ml-2"></i></button>
                            </div>
                           
                        </form>
