            <!-- /page header -->
            <!-- Content area -->
            <div class="content">
                <!-- Basic initialization -->
                <div class="card">
                    <div class="card-header header-elements-inline">
                        <h5 class="card-title">View Save Reports</h5>

                        <div class="header-elements">

                            <div class="list-icons">
                                <a class="list-icons-item" data-action="collapse"></a>
                                <a class="list-icons-item" data-action="reload"></a>
                                <a class="list-icons-item" data-action="remove"></a>
                            </div>

                        </div>
                    </div>
                    <div class="card-header header-elements-inline">
                        <div class="col-md-12">
                            <div class="form-group">
                                
                                <input type="search" ng-model="search" placeholder="Search" />

                            </div>

                        </div>


                    </div>
                    <table class="table table-bordered table-hover datatable-highlight">
                        <thead>
                            <tr>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
                <!-- /basic initialization -->
                <!-- Basic initialization -->
                <div class="card">
                    <div class="card-header header-elements-inline">
                        <h5 class="card-title">Reports</h5>

                        <div class="header-elements">
                            <input type="text" ng-model="reports.report_name" placeholder="Save View Name"> &nbsp;
                            <button class="btn btn-dark" value="{{btntext}}" ng-click="savedatare()">Save View</button> &nbsp;
                            <button class="btn btn-danger" ng-click="PdfReportsp()">pdf</button> &nbsp;
                            <button class="btn btn-success " ng-click="ExcelReportsp()">Excel</button>
                            &nbsp;
                            <div class="list-icons">
                                <a class="list-icons-item" data-action="collapse"></a>
                                <a class="list-icons-item" data-action="reload"></a>
                                <a class="list-icons-item" data-action="remove"></a>
                            </div>

                        </div>
                    </div>
                    <div class="card-header header-elements-inline">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="d-block font-weight-semibold">Filters</label>
                                <input type="search" ng-model="search" placeholder="Search" />
                                &nbsp;
                                <input type="checkbox" value="supplier_id" ng-model="reports.supplier_id">
                                <label>Supplier Id</label>
                                &nbsp;
                                <input type="checkbox" value="supplier_name" ng-model="reports.supplier_name" >
                                <label>Supplier Name</label>
                                &nbsp;
                                <input type="checkbox" value="account_number" ng-model="reports.account_number" >
                                <label>Account Number</label>
                                &nbsp;&nbsp;
                                <input type="checkbox" value="email" ng-model="reports.email" >
                                <label>Email</label>
                                &nbsp;&nbsp;
                                <input type="checkbox" value="address" ng-model="reports.address" >
                                <label>Address</label>
                                &nbsp;
                                <input type="checkbox" value="document_types" ng-model="reports.document_types" >
                                <label>Document Type</label>
                                &nbsp;
                                <input type="checkbox" value="comments" ng-model="reports.comments">
                                <label>Comments</label>
                                &nbsp;
                                <input type="checkbox" value="rules_type" ng-model="reports.rules_type" >
                                <label>Rules Type</label>
                                &nbsp;
                                <input type="checkbox" value="rules" ng-model="reports.rules" >
                                <label>Rules</label>
                            </div>

                        </div>


                    </div>
                    <table class="table table-bordered table-hover datatable-highlight">
                        <thead>
                            <tr>
                                <th ng-if="reports.supplier_id">Supplier Id</th>
                                <th ng-if="reports.supplier_name">Supplier Name</th>
                                <th ng-if="reports.account_number">Account Number</th>
                                <th ng-if="reports.email">Email</th>
                                <th ng-if="reports.address">Address</th>
                                <th ng-if="reports.document_types">Document Type</th>
                                <th ng-if="reports.comments">Comments</th>
                                <th ng-if="reports.rules_type">Rule Type</th>
                                <th ng-if="reports.rules">Rules</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="e in supplieronboard |filter:search ">
                                <td ng-if="reports.supplier_id">{{e.supplier_id}}</td>
                                <td ng-if="reports.supplier_name">{{e.supplier_name}}</td>
                                <td ng-if="reports.account_number">{{e.account_number}}</td>
                                <td ng-if="reports.email">{{e.email}}</td>
                                <td ng-if="reports.address">{{e.address}}</td>
                                <td ng-if="reports.document_types">{{e.document_types}}</td>
                                <td ng-if="reports.comments">{{e.comments}}</td>
                                <td ng-if="reports.rules_type">{{e.rules_type}}</td>
                                <td ng-if="reports.rules">{{e.rules}}</td>
                            </tr>

                        </tbody>
                    </table>
                </div>
